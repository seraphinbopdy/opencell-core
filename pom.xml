				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>license-maven-plugin</artifactId>
						<version>2.4.0</version>
						<configuration>
							<useMissingFile>true</useMissingFile>
							<!-- assuming that the file is in the same dir as
							the pom.xml -->
